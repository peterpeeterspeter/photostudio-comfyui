{
  "1": {
    "inputs": {
      "image": "test_garment.jpg"
    },
    "class_type": "LoadImage",
    "_meta": {
      "title": "Load Garment Image"
    }
  },
  "2": {
    "inputs": {
      "image": ["1", 0],
      "threshold": 0.30,
      "mask_blur": 3,
      "post_dilate": 8,
      "output_mode": "both",
      "model_name": "u2net"
    },
    "class_type": "RMBGNode",
    "_meta": {
      "title": "RMBG Segmentation"
    }
  },
  "3": {
    "inputs": {
      "image": ["1", 0],
      "threshold": 0.35,
      "post_process": true,
      "edge_refinement": true,
      "model_name": "u2net"
    },
    "class_type": "U2NetSegmentation",
    "_meta": {
      "title": "UÂ²-Net Segmentation"
    }
  },
  "4": {
    "inputs": {
      "mask1": ["2", 1],
      "mask2": ["3", 1],
      "blend_mode": "weighted",
      "mask1_weight": 0.4,
      "mask2_weight": 0.6,
      "edge_feather": 4,
      "dilate": 8
    },
    "class_type": "MaskEnsemble",
    "_meta": {
      "title": "Mask Ensemble"
    }
  },
  "5": {
    "inputs": {
      "images": ["2", 0],
      "filename_prefix": "test_mask_ensemble"
    },
    "class_type": "SaveImage",
    "_meta": {
      "title": "Save RMBG Image"
    }
  }
