    "class_type": "KSampler",
    "_meta": {
      "title": "KSampler (SDXL/FLUX)"
    }
  },
  "20": {
    "inputs": {
      "width": 1024,
      "height": 1024,
      "batch_size": 1
    },
    "class_type": "EmptyLatentImage",
    "_meta": {
      "title": "Empty Latent Image"
    }
  },
  "21": {
    "inputs": {
      "samples": ["19", 0],
      "vae": ["5", 1]
    },
    "class_type": "VAEDecode",
    "_meta": {
      "title": "VAE Decode"
    }
  },
  "22": {
    "inputs": {
      "image": ["21", 0],
      "mask": ["2", 1],
      "prompt": ["13", 0],
      "negative_prompt": ["13", 1],
      "model": ["5", 0],
      "vae": ["5", 1],
      "clip": ["5", 2],
      "controlnet": ["17", 0],
      "controlnet_image": ["16", 0],
      "strength": 0.3,
      "steps": 15,
      "cfg": 7.0
