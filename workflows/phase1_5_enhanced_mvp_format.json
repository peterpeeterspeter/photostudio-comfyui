{
  "1": {
    "inputs": {
      "image": "test_garment.jpg"
    },
    "class_type": "LoadImage",
    "_meta": {
      "title": "Load Garment Image"
    }
  },
  "2": {
    "inputs": {
      "facts_file_path": "test_garment_facts.json"
    },
    "class_type": "LoadFactsNode",
    "_meta": {
      "title": "Load Light Facts"
    }
  },
  "3": {
    "inputs": {
      "image": ["1", 0],
      "threshold": 0.30,
      "mask_blur": 3,
      "post_dilate": 8,
      "output_mode": "both",
      "model_name": "u2net"
    },
    "class_type": "RMBGNode",
    "_meta": {
      "title": "RMBG Segmentation"
    }
  },
  "4": {
    "inputs": {
      "image": ["1", 0],
      "threshold": 0.35,
      "post_process": true,
      "edge_refinement": true,
      "model_name": "u2net"
    },
    "class_type": "U2NetSegmentation",
    "_meta": {
      "title": "U²-Net Segmentation"
    }
  },
  "5": {
    "inputs": {
      "mask1": ["3", 1],
      "mask2": ["4", 1],
      "blend_mode": "weighted",
      "mask1_weight": 0.4,
      "mask2_weight": 0.6,
      "edge_feather": 4,
      "dilate": 8
    },
    "class_type": "MaskEnsemble",
    "_meta": {
      "title": "Mask Ensemble (RMBG + U²-Net)"
    }
  },
  "6": {
    "inputs": {
      "facts_json": ["2", 1],
      "prompt_style": "detailed",
      "include_constraints": true
    },
    "class_type": "PromptFromLightFacts",
    "_meta": {
      "title": "Generate Prompt from Facts"
    }
  },
  "7": {
    "inputs": {
      "ckpt_name": "sd_xl_base_1.0.safetensors"
    },
    "class_type": "CheckpointLoaderSimple",
    "_meta": {
      "title": "Load SDXL Model"
    }
  },
  "8": {
    "inputs": {
      "clip": ["7", 1],
      "text": ["6", 0]
    },
    "class_type": "CLIPTextEncode",
    "_meta": {
      "title": "Encode Positive Prompt"
    }
  },
  "9": {
    "inputs": {
      "clip": ["7", 1],
      "text": ["6", 1]
    },
    "class_type": "CLIPTextEncode",
    "_meta": {
      "title": "Encode Negative Prompt"
    }
  },
  "10": {
    "inputs": {
      "width": 1024,
      "height": 1024,
      "batch_size": 1
    },
    "class_type": "EmptyLatentImage",
    "_meta": {
      "title": "Create Empty Latent"
    }
  },
  "11": {
    "inputs": {
      "model": ["7", 0],
      "positive": ["8", 0],
      "negative": ["9", 0],
      "latent_image": ["10", 0],
      "seed": 123456,
      "steps": 20,
      "cfg": 7.0,
      "sampler_name": "euler",
      "scheduler": "normal",
      "denoise": 1.0
    },
    "class_type": "KSampler",
    "_meta": {
      "title": "Generate Ghost Mannequin"
    }
  },
  "12": {
    "inputs": {
      "samples": ["11", 0],
      "vae": ["7", 2]
    },
    "class_type": "VAEDecode",
    "_meta": {
      "title": "Decode to Image"
    }
  },
  "13": {
    "inputs": {
      "images": ["12", 0],
      "filename_prefix": "phase1_5_enhanced_ghost"
    },
    "class_type": "SaveImage",
    "_meta": {
      "title": "Save Final Image"
    }
  }
}
